<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    helpPath="en/cq/current/wcm/default_components.html#Text"
    title="Text"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab1
            jcr:primaryType="cq:Widget"
            anchor="100%"
            title="Text"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <text
                    jcr:primaryType="cq:Widget"
                    hideLabel="{Boolean}true"
                    name="./text"
                    externalStyleSheets="[]"
                    xtype="richtext">
                    <rtePlugins 
                        jcr:primaryType="nt:unstructured">
					    <edit           jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/edit.infinity.json"/>
					    <findreplace    jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/findreplace.infinity.json"/>
					    <format         jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/format.infinity.json"/>
					    <image          jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/image.infinity.json"/>
					    <justify        jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/image.infinity.json"/>
					    <keys           jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/justify.infinity.json"/>
					    <links          jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/links.infinity.json"/>
					    <lists          jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/lists.infinity.json"/>
					    <misctools      jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/misctools.infinity.json"/>
					    <paraformat     jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/paraformat.infinity.json"/>
					    <subsuperscript jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/subsuperscript.infinity.json"/>
					    <table          jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/table.infinity.json"/>
					    <undo           jcr:primaryType="cq:Widget" xtype="cqinclude" path="/apps/ironsites/common/rtePlugins/undo.infinity.json"/>
                        <styles         jcr:primaryType="nt:unstructured" features="*">
                            <styles
                                jcr:primaryType="cq:Widget"
                                path="/bin/options/tag.json?path=/etc/tags/ironsites/styles/components/text"
                                xtype="cqinclude"/>
	                   </styles>
	               </rtePlugins>
                </text>
                <isRichTextFlag
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./textIsRich"
                    value="true"
                    xtype="hidden"/>
            </items>
        </tab1>
    </items>
</jcr:root>
