<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="ironsites three column template"
    jcr:mixinTypes="[mix:lockable]"
    jcr:primaryType="cq:Template"
    jcr:title="ironsites Three Column Template"
    allowedPaths="[/content(/.*)+]"
    ranking="{Long}2">
    <jcr:content
        cq:designPath="/etc/designs/ironsites"
        jcr:primaryType="cq:PageContent"
        layout="{Long}3"
        sling:resourceType="ironsites/components/page/general"
        cq:deviceGroups="[/etc/mobile/groups/responsive]">
        <par 
	        jcr:primaryType="nt:unstructured"
	        sling:resourceType="wcm/foundation/compoennts/responsivegrid">
	        <cq:responsive 
		        jcr:primaryType="nt:unstructured">
		        <breakpoints
		        	jcr:primaryType="nt:unstructured">           
                    <small
                        jcr:primaryType="nt:unstructured"
                        title="Small (Phone)"
                        width="768" />              
                    <medium
                        jcr:primaryType="nt:unstructured"
                        title="Medium (Tablet)"
                        width="1200"/>
	        	</breakpoints>
			</cq:responsive>
		</par>
        <leftpar 
	        jcr:primaryType="nt:unstructured"
	        sling:resourceType="wcm/foundation/compoennts/responsivegrid">
	        <cq:responsive 
		        jcr:primaryType="nt:unstructured">
		        <breakpoints
		        	jcr:primaryType="nt:unstructured">           
                    <small
                        jcr:primaryType="nt:unstructured"
                        title="Small (Phone)"
                        width="768" />              
                    <medium
                        jcr:primaryType="nt:unstructured"
                        title="Medium (Tablet)"
                        width="1200"/>
	        	</breakpoints>
			</cq:responsive>
		</leftpar>
        <rightpar 
	        jcr:primaryType="nt:unstructured"
	        sling:resourceType="wcm/foundation/compoennts/responsivegrid">
	        <cq:responsive 
		        jcr:primaryType="nt:unstructured">
		        <breakpoints
		        	jcr:primaryType="nt:unstructured">           
                    <small
                        jcr:primaryType="nt:unstructured"
                        title="Small (Phone)"
                        width="768" />              
                    <medium
                        jcr:primaryType="nt:unstructured"
                        title="Medium (Tablet)"
                        width="1200"/>
	        	</breakpoints>
			</cq:responsive>
		</rightpar>
    </jcr:content>
</jcr:root>
